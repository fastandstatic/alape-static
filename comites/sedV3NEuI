<script>$(window).scroll(function () {
      if($(window).scrollTop() > 20) {
        $("#head").addClass('sticky');
        $("#head-child").addClass('sticky-head');
      } else {
        $("#head").removeClass('sticky');
        $("#head-child").removeClass('sticky-head');
      }
    });
    
    
    $(document).ready(function() {
        $('.btn_personalizado a').hover(
            function() {
                $(this).find('span.elementor-button-text').addClass('cambiar_text');
            },
            function() {
                $(this).find('span.elementor-button-text').removeClass('cambiar_text');
            }
        );
    });</script>
